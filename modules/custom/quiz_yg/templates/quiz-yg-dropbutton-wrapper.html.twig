{#
/**
 * @file
 * Default theme implementation for a quiz_yg dropbutton wrapper.
 *
 * Available variables:
 * - children: Contains the child elements of the quiz_yg dropbutton menu.
 *
 * @see template_preprocess()
 * @see template_preprocess_quiz_yg_dropbutton_wrapper()
 *
 * @ingroup themeable
 */
#}
{% if children %}
  {% spaceless %}
    <div class="quiz_yg-dropbutton-wrapper">
      {{ children }}
    </div>
  {% endspaceless %}
{% endif %}
