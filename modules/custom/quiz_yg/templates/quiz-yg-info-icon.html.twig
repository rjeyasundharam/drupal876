{#
/**
 * @file
 * Default theme implementation for a quiz_yg info icon.
 *
 * Available variables:
 * - icon: Name of the icon to use.
 * - message: Information message.
 *
 * @see template_preprocess()
 *
 * @ingroup themeable
 */
#}
<span class="quiz_yg-icon quiz_yg-icon-{{ icon }}" title="{{ message }}">
  <span class="quiz_yg-icon__message visually-hidden">{{ message }}</span>
</span>
